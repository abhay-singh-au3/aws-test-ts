{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "resolveJsonModule":true,
      "target": "es6",
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist",
      "skipLibCheck": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true
    },
    "lib": ["es5", "es6"],
    "exclude": [
      "client",
    ]
}